@{
Layout = null;
}
<!doctype html>
<html lang="zh-cn">
	<head>
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
		<meta charset="UTF-8">
		<title>Draw Together</title>
		<script type="text/javascript" src="~/Scripts/jQuery/jquery-2.1.1.min.js"></script>
		<script type="text/javascript" src="~/Scripts/jQuery/bootstrap.min.js"></script>
		<script type="text/javascript" src="~/Scripts/jQuery/jquery.alert.js"></script>
		<script type="text/javascript" src="~/Scripts/jQuery/jquery.websocket.js"></script>
		<script type="text/javascript" src="~/Scripts/easeljs-min.js"></script>
		<script type="text/javascript" src="~/Scripts/DTcore.js"></script>

		<link rel="stylesheet" href="~/content/css/bootstrap.min.css">
		<link rel="stylesheet" href="~/content/css/bootstrap-theme.min.css">
		<link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
		<link rel="stylesheet" href="~/content/css/global.css">
		<link rel="stylesheet" href="~/content/css/window.css">
	</head>
	<body>
		<div class="slide-menu">
			<div class="slide-menu-logo">
				<img src="~/Content/image/DTlogo.png" width="50" alt="logo">
			</div>

			<div class="slide-menu-list">
				<div class="form-group">
					<div class="btn-group-vertical">
						<button type="button" class="btn btn-default pen-color" data-property="red">红色</button>
						<button type="button" class="btn btn-default active pen-color" data-property="black">黑色</button>
						<button type="button" class="btn btn-default pen-color" data-property="blue">蓝色</button>
					</div>
				</div>
				<div class="form-group">
					<div class="btn-group-vertical">
						<button type="button" class="btn btn-default pen-thickness" data-property="10">粗</button>
						<button type="button" class="btn btn-default pen-thickness active" data-property="5">中</button>
						<button type="button" class="btn btn-default pen-thickness" data-property="1">细</button>
					</div>
				</div>
				<div class="form-group">
					<div class="btn-group-vertical">
						<button type="button" class="btn btn-default" id="clearShape">清除</button>
						
					</div>
				</div>
			</div>
			<div class="list-group slide-menu-footer">
				<a href="/Account/Signout" class="list-group-item text-center" title="注销"><span class="fa fa-sign-out fa-lg"></span></a>
			</div>
		</div>
		<div class="canvas-container">
			<canvas id="canvas"></canvas>
			<!-- <div class="toolbar">
				<div class="btn-toolbar" role="toolbar">
					<div class="btn-group">
						<button class="btn btn-default pen-color" data-property="red"><span class="text-red">红色</span></button>
						<button class="btn btn-primary pen-color" data-property="black"><span class="text-black">黑色</span></button>
						<button class="btn btn-default pen-color" data-property="blue"><span class="text-blue">蓝色</span></button>
					</div>
					<div class="btn-group">
						<button class="btn btn-default pen-thickness" data-property="10">粗</button>
						<button class="btn btn-primary pen-thickness" data-property="5">中</button>
						<button class="btn btn-default pen-thickness" data-property="1">细</button>
					</div>
				</div>
			</div> -->
		</div>
		<div class="slide-user-list">
			<div class="list-group">
				<a class="list-group-item active"><span class="fa fa-list"></span> 用户列表</a>
				<a id="userInfo" data-userid="@ViewBag.UserId" href="javascript:;" class="list-group-item list-group-item-info">@ViewBag.UserName</a>
			</div>
			<div class="copyright">Copyright © 2014-2015</div>
			<div class="copyright">Fisher Chang</div>
		</div>
	</body>
</html>